{
  "name": "@vert/services",
  "version": "0.1.2",
  "description": "Prebuilt services for Vert.",
  "author": {
    "name": "LancerComet",
    "email": "chw644@hotmail.com"
  },
  "main": "./lib/index.js",
  "module": "./src/index.ts",
  "types": "./types/index.d.ts",
  "typings": "./types/index.d.ts",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/LancerComet/Vert-Services",
    "type": "git"
  },
  "scripts": {
    "build": "webpack --config ./build/webpack.build.conf.js",
    "test": "jest",
    "preversion": "npm test",
    "prepublish": "npm run build"
  },
  "files": [
    "doc/",
    "lib/",
    "types/"
  ],
  "dependencies": {
    "@vert/core": "^0.1.3",
    "axios": "0.17.1",
    "qs": "6.5.1",
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@types/jest": "22.0.1",
    "@types/node": "9.4.5",
    "babel-core": "6.26.0",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.9",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.1.4",
    "regenerator-runtime": "0.11.1",
    "ts-jest": "22.0.1",
    "ts-loader": "3.5.0",
    "typescript": "2.8.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1"
  },
  "keywords": [
    "Framework",
    "Vue",
    "Vert"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "transform": {
      ".+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      ".+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(spec|test).(js|ts)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.jest/file-mock.js",
      "\\.(css|less)$": "<rootDir>/.jest/style-mock.js"
    },
    "mapCoverage": true,
    "setupFiles": [
      "<rootDir>/.jest/setup.js"
    ],
    "coverageDirectory": "<rootDir>/.coverage-report",
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ]
  }
}
